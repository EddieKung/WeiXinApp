.swiper-index swiper {
  height: 340rpx;
}
.swiper-index swiper image {
  width: 100%;
}
.cate-index {
  display: flex;
  padding: 30rpx;
}
.cate-index navigator {
  flex: 1;
  padding: 10rpx;
}
.cate-index navigator image {
  width: 100%;
}
.floor-index .floor-list .floor-title {
  padding: 10rpx 0;
}
.floor-index .floor-list .floor-content {
  overflow: hidden;
}
.floor-index .floor-list .floor-content navigator {
  float: left;
  width: 33.33%;
}
.floor-index .floor-list .floor-content navigator:nth-last-child(-n+4) {
  height: 27.72711207vw;
  border-left: 10rpx solid #fff;
}
.floor-index .floor-list .floor-content navigator:nth-child(2),
.floor-index .floor-list .floor-content navigator:nth-child(3) {
  border-bottom: 10rpx solid #fff;
}
.floor-index .floor-list .floor-content navigator image {
  height: 100%;
}
